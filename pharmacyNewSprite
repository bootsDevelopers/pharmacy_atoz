<script type="text/javascript">
$(document).ready(function() {
	$('.atozList').hide();
	$('.listAF').show();
	$('.atozLinkAF').addClass('atozActiveBut');
	$('.listAF').addClass('atozActiveList');

	function fadeAtozClickAF(){
		$('.atozList').removeClass('atozActiveList');
		$('.listAF').addClass('atozActiveList');
		$('.atozList').hide();
		$('.listAF').show();
		$('.atozMenu a').removeClass('atozActiveBut');
		$('.atozLinkAF').addClass('atozActiveBut');
	}
	function fadeAtozClickGP(){
		$('.atozList').removeClass('atozActiveList');
		$('.listGP').addClass('atozActiveList');
		$('.atozList').hide();
		$('.listGP').show();
		$('.atozMenu a').removeClass('atozActiveBut');
		$('.atozLinkGP').addClass('atozActiveBut');
	}
	function fadeAtozClickQZ(){
		$('.atozList').removeClass('atozActiveList');
		$('.listQZ').addClass('atozActiveList');
		$('.atozList').hide();
		$('.listQZ').show();
		$('.atozMenu a').removeClass('atozActiveBut');
		$('.atozLinkQZ').addClass('atozActiveBut');
	}

	$('.atozLinkAF').click(function(){
		fadeAtozClickAF()
		return false;
	});

	$('.atozLinkGP').click(function(){
		fadeAtozClickGP()
		return false;
	});

	$('.atozLinkQZ').click(function(){
		fadeAtozClickQZ()
		return false;
	});
});
</script>
